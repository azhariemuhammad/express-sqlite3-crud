<!-- views/pages/index.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
</head>
<body class="container">

    <header>
        <% include ../partials/header %>
    </header>

    <main>
        <div>
        <% temp.forEach(function(temps) { %>
            <form action="/editContactFinal" method="POST">
                <input type="text" placeholder=<%= temps.id%> name="id">
                <input type="text" placeholder=<%= temps.name%> name="name">
                <input type="text" placeholder=<%= temps.company%> name="company">
                <input type="text" placeholder=<%= temps.telp%> name="telp">
                <input type="text" placeholder=<%= temps.email%> name="email">
                <button type="submit">Submit</button>
            </form>
        <% }); %>
        </div>
    </main>

    <footer>
        <% include ../partials/footer %>
    </footer>

</body>
</html>
